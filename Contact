package addressbook;

public class Contact {
	String name;
	String tel;
	String address;
	
	
	
	public Contact() {}


	public Contact(String name, String tel, String address) {
		this.name = name;
		this.tel = tel;
		this.address = address;
	}



	public String get() {
		return String.format("이름: %s , 연락처: %s , 사는곳: %s", name,tel,address);
	}
	
	public void print() {
		System.out.println(get());
	}
	

	
}
